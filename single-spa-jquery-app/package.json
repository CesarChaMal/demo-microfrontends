    "lint": "eslint src --ext .js --fix"
  },
  "devDependencies": {
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "eslint": "^6.8.0",
