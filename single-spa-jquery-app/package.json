{
  "name": "@cesarchamal/single-spa-jquery-app",
  "version": "0.1.3",
  "description": "jQuery microfrontend for Single-SPA demonstrating legacy library integration",
  "main": "dist/single-spa-jquery-app.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "start": "webpack serve --mode development --port 4210",
