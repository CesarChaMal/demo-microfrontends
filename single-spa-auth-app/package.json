{
  "name": "@cesarchamal/single-spa-auth-app",
  "version": "0.1.1",
  "description": "Vue application with login form for be included in a single-spa application as registered app.",
  "main": "dist/single-spa-auth-app.umd.js",
  "scripts": {
    "build": "vue-cli-service build --target lib --formats umd src/singleSpaEntry.js",
    "build:dev": "vue-cli-service build --target lib --formats umd src/singleSpaEntry.js --mode development",
    "build:prod": "vue-cli-service build --target lib --formats umd src/singleSpaEntry.js --mode production",
    "serve": "npm run build && npx http-server dist -p 4201 --cors",
    "lint": "vue-cli-service lint"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.1.0",
    "@vue/cli-plugin-eslint": "4.1.0",
    "@vue/cli-service": "4.1.0",
    "babel-eslint": "10.0.3",
    "core-js": "3.4.4",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-vue": "5.0.0",
    "sass-loader": "8.0.2",
    "vue-cli-plugin-single-spa": "1.1.0",
    "vue-eslint-parser": "^6.0.0",
    "vue-template-compiler": "2.6.11",
    "webpack": "4.41.5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cesarchamal/single-spa-auth-app.git"
  },
  "keywords": [
    "single-spa",
    "login",
    "npm",
    "webpack",
    "vue",
    "bootstrap-vue",
    "bootstrap"
  ],
  "author": "Cesar Francisco Chavez Maldonado",
  "bugs": {
    "url": "https://github.com/cesarchamal/single-spa-auth-app/issues"
  },
  "homepage": "https://github.com/cesarchamal/single-spa-auth-app#readme",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "sass": "^1.77.8",
    "vue": "^2.6.11",
    "vue-google-adsense": "1.9.2",
    "vue-script2": "2.1.0",
    "vue-toastr": "2.1.2",
    "single-spa-vue": "1.7.0",
    "bootstrap-vue": "2.2.2",
    "@fortawesome/fontawesome-svg-core": "1.2.27",
    "@fortawesome/free-brands-svg-icons": "5.12.1",
    "@fortawesome/free-solid-svg-icons": "5.12.0",
    "@fortawesome/vue-fontawesome": "0.1.9",
    "vue-router": "3.1.4"
  }
}
